set terminal pdf
set output "eil51.pdf"
set xrange [0:100]
set yrange [521:1200]
set xtics 30

set style line 1 \
    linecolor rgb '#0060ad' \
    linetype 1 linewidth 1 \
    pointtype 7 pointsize 0.5
set style line 2 \
    linecolor rgb '#dd181f' \
    linetype 1 linewidth 1 \
    pointtype 7 pointsize 0.5

plot 'data.dat' using 1:2 with linespoints ls 1 title "Best",\
     'data.dat' using 1:3 with linespoints ls 2 title "Worst"
